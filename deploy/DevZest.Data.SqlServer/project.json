{
  "version": "0.0.0-dev",
  "description": "DevZest.Data.SqlServer",
  "authors": [ "Weifen Luo | DevZest" ],
  "packOptions": {
    "requireLicenseAcceptance": true,
    "licenseUrl": "http://www.devzest.com",
    "repository": {
      "type": "git",
      "url": "http://www.devzest.com"
    },
    "tags": [ "RDO.NET", "ADO.NET" ]
  },
  "buildOptions": {
    "compile": {
      "include": "../../src/Data.SqlServer/**/*.cs"
    },
    "embed": {
      "include": [ "../../src/Data.SqlServer/Properties/*.resx" ],
      "mappings": {
        "DevZest.Data.SqlServer": "../../src/Data.SqlServer/Properties"
      }
    },
    "warningsAsErrors": true,
    "define": [ "DEPLOY" ],
    "keyFile": "../Key.snk"
  },
  "dependencies": {
    "DevZest.Data.Common": {
      "target": "package",
      "version": "0.0.0-dev"
    }
  },
  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "Microsoft.NETCore.Portable.Compatibility": "1.0.1",
        "NETStandard.Library": "1.6.0",
        "System.Data.SqlClient": "4.1.0"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0",
        "System.Xml": "4.0.0.0"
      }
    }
  }
}
