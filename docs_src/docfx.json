{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [ 
            "Data/Data.csproj",
            "Data.SqlServer/Data.SqlServer.csproj",
            "Data.MySql/Data.MySql.csproj",
            "Data.WPF/Data.WPF.csproj",
            "Data.AspNetCore/Data.AspNetCore.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": { "TargetFramework": "netstandard2.0" },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© Weifen Luo | DevZest</span>"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "nuget_packages/memberpage.2.41.0/content"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}