<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:DevZest.Windows.Data"
    xmlns:primitives="clr-namespace:DevZest.Windows.Data.Primitives">
    
    <Style TargetType="{x:Type local:RowView}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:RowView}">
                    <primitives:RowViewPanel x:Name="PART_Panel" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
