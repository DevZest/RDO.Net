  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\..\external\Utilities\src\Utilities\Argument.VerifyNotNull.cs" Link="Utilities\Argument.VerifyNotNull.cs" />
    <Compile Include="..\..\external\Utilities\src\Utilities\ArgumentList.VerifyNotEmpty.cs" Link="Utilities\ArgumentList.VerifyNotEmpty.cs" />
    <Compile Include="..\..\external\Utilities\src\Utilities\ArgumentListItem.VerifyNotNull.cs" Link="Utilities\ArgumentListItem.VerifyNotNull.cs" />
    <Compile Include="..\..\external\Utilities\src\Utilities\ArgumentString.VerifyNotEmpty.cs" Link="Utilities\ArgumentString.VerifyNotEmpty.cs" />
